\begin_layout Paragraph
\begin_inset Formula 
\[
I_{n}=\intop_{-1}^{1}x^{n}e^{-x}dx,\quad n=0,..,20
\]

\end_inset


\series bold
Convergence de l'intégrale
\end_layout

\begin_layout Paragraph
\begin_inset Formula 
\begin{align*}
\begin{vmatrix}I_{n}\end{vmatrix} & =\begin{vmatrix}\intop_{-1}^{1}x^{n}e^{-x}dx\end{vmatrix}\leq\intop_{-1}^{1}|x^{n}|e^{-x}dx\\
 & \le e^{1}\times\intop_{-1}^{1}\begin{vmatrix}x^{n}\end{vmatrix}dx=e^{1}\times\left(\intop_{-1}^{0}(-x)^{n}dx+\intop_{0}^{1}x^{n}dx\right)\\
 & =2e^{1}\times\intop_{0}^{1}x^{n}dx=2e^{1}\times\left[\frac{x^{n+1}}{n+1}\right]_{0}^{1}\\
 & =2e^{1}\times\frac{1}{n+1}\xrightarrow[n\rightarrow+\infty]{}0
\end{align*}

\end_inset


\series bold
Relation de récurrence
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
I_{0}= & \intop_{-1}^{1}e^{-x}dx\\
= & \left[e^{-x}\right]_{1}^{-1}=e^{1}-e^{-1}\\
= & e^{1}-e^{-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph
\begin_inset Formula 
\begin{align*}
I_{n} & =\intop_{-1}^{1}x^{n}e^{-x}dx\\
 & =\left[-x^{n}e^{-x}\right]_{-1}^{1}+\intop_{-1}^{1}nx^{n-1}e^{-x}dx\\
 & =-(1)^{n}e^{-1}+(-1)^{n}e^{1}+n\intop_{-1}^{1}x^{n-1}e^{-x}dx\\
 & =nI_{n-1}-e^{-1}+(-1)^{n}e^{1}
\end{align*}

\end_inset


