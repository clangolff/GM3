\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \begin{cases}
r_{1}(s,p) & =0\\
r_{0}(s,p) & =0
\end{cases}\\
\iff & \begin{cases}
a_{1}+sq_{0}-pq_{1}(s,p) & =0\\
a_{0}-pq_{1}(s,p) & =0
\end{cases}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Rappel : 
\begin_inset Formula $F:\mathbb{R}^{2}→\mathbb{R}^{2}$
\end_inset

, 
\begin_inset Formula 
\begin{align*}
x_{n+1} & =x_{n}-\left[J_{x_{n}}F\right]^{-1}F(x_{n})\\
\iff x_{n+1} & =x_{n}+\left[\begin{matrix}\frac{\partial F_{1}}{\partial x_{1}}(x^{n}) & \frac{\partial F_{1}}{\partial x_{2}}(x^{n})\\
\frac{\partial F_{2}}{\partial x_{1}}(x^{n}) & \frac{\partial F_{2}}{\partial x_{2}}(x^{n})
\end{matrix}\right]^{-1}\left[\begin{matrix}F_{1}(x_{n})\\
F_{2}(x_{n})
\end{matrix}\right]
\end{align*}

\end_inset


\end_layout

