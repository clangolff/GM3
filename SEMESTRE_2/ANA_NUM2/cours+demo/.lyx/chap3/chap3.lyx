\series bold
Preuve
\end_layout

\begin_layout Standard
Dire que 
\begin_inset Formula $G$
\end_inset

 est différentiable en 
\begin_inset Formula $x^{*}$
\end_inset

 est équivalent à dire que 
\begin_inset Formula $G$
\end_inset

 est continue en 
\begin_inset Formula $x^{*}$
\end_inset


\begin_inset Formula 
\begin{align*}
\iff & lim_{x→x^{*}}d_{x}G=d_{x^{*}}G\\
\iff & \forall\epsilon>0,\exists\eta>0\;tq\;\bigparallel x-x^{*}\bigparallel<\eta\implies\bigparallel d_{x}G-d_{x^{*}G}\bigparallel<\epsilon\\
\iff & d_{x}G\in B(d_{x^{*}}G,\epsilon)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Posons 
\begin_inset Formula $l=1-\bigparallel d_{x}G\bigparallel$
\end_inset

 et prenons 
\begin_inset Formula $\epsilon<l$
\end_inset

 alors 
\begin_inset Formula $\exists\eta$
\end_inset

 tq 
\begin_inset Formula $\bigparallel x-x^{*}\bigparallel<\eta\implies\bigparallel d_{x}G\bigparallel<\bigparallel d_{x^{*}}G\bigparallel+\epsilon<1$
\end_inset


\end_layout

\begin_layout Standard
car 
\begin_inset Formula $\bigparallel d_{x}G\bigparallel=\bigparallel d_{x}G+d_{x^{*}}G-d_{x^{*}}G\bigparallel<\bigparallel d_{x^{*}}G\bigparallel+\bigparallel d_{x}G-d_{x^{*}}G\bigparallel<\bigparallel d_{x^{*}}G\bigparallel+\epsilon<1$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $\forall x,y\in E$
\end_inset

 ,
\begin_inset Formula 
\begin{align*}
\bigparallel G(x)-G(y)\bigparallel & =\bigparallel G(x-y+y)-G(y)\bigparallel\\
 & =\bigparallel G(y)-G(y)+d_{y}G(x-y)+\Phi_{y}(x-y)\bigparallel
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
où 
\begin_inset Formula $\Phi_{y}(h)$
\end_inset

 est tel que 
\begin_inset Formula $lim_{h→0}\frac{\Phi_{y}(h)}{\bigparallel h\bigparallel}=0$
\end_inset


\end_layout

\begin_layout Standard
Ainsi 
\begin_inset Formula 
\begin{align*}
\bigparallel G(x)-G(y)\bigparallel & \leq\bigparallel d_{y}G\bigparallel\bigparallel x-y\bigparallel+\frac{\Phi_{y}(x-y)}{\bigparallel x-y\bigparallel}\bigparallel x-y\bigparallel\\
 & \leq(\bigparallel d_{y}G\bigparallel+\frac{\Phi_{y}(x-y)}{\bigparallel x-y\bigparallel})\bigparallel x-y\bigparallel
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
On a 
\begin_inset Formula $\forall y\in B^{f}(x^{*},\eta)$
\end_inset

 , 
\begin_inset Formula $\bigparallel d_{y}G\bigparallel<1$
\end_inset


\end_layout

\begin_layout Standard
Notons 
\begin_inset Formula $max_{y\in B^{f}(x^{*},\eta)}\bigparallel d_{y}G\bigparallel=M^{d}<1$
\end_inset


\end_layout

\begin_layout Standard
posons 
\begin_inset Formula $\widetilde{\epsilon}=\frac{1-M^{d}}{2}>0$
\end_inset

 alors 
\begin_inset Formula $\exists\widetilde{\eta}$
\end_inset

 tel que 
\begin_inset Formula $\bigparallel x-y\bigparallel<\widetilde{\eta}\implies\frac{\Phi_{y}(x-y)}{\bigparallel x-y\bigparallel}\bigparallel<\widetilde{\epsilon}$
\end_inset


\end_layout

\begin_layout Standard
Pour résumer, si 
\begin_inset Formula $(x,y)\in\left(B(x^{*},min(\eta,\widetilde{\eta}))\right)^{2}$
\end_inset

 alors 
\begin_inset Formula $\bigparallel G(x)-G(y)\bigparallel\leq k\bigparallel x-y\bigparallel$
\end_inset

 où 
\begin_inset Formula $k=M^{d}+\widetilde{\epsilon}<1$
\end_inset

 
\end_layout

\begin_layout Standard
Ainsi, on aura 
\begin_inset Formula 
\[
G:B(x^{*},min(\eta,\widetilde{\eta}))→B(x^{*},min(\eta,\widetilde{\eta}))
\]

\end_inset

 qui sera une contraction et par le théorème du point fixe, on déduit que
 
\begin_inset Formula $x_{n+1}=G(x_{n})→x^{*}$
\end_inset

 si 
\begin_inset Formula $x_{0}\in B(x^{*},min(\eta,\widetilde{\eta}))$
\end_inset

 
\end_layout

\begin_layout Standard
Notons que 
\begin_inset Formula $\forall x\in B(x^{*},min(\eta,\widetilde{\eta}))$
\end_inset

 , 
\begin_inset Formula $\bigparallel G(x^{*})-x^{*}\bigparallel\leq\underbrace{(\bigparallel d_{x^{*}}G\bigparallel+\frac{\Phi_{y}(x-x^{*})}{\bigparallel x-x^{*}\bigparallel})}_{<1}\underbrace{\bigparallel x-x^{*}\bigparallel}_{\leq min(\eta,\widetilde{\eta})}$
\end_inset


\end_layout

\begin_layout Standard
d'où 
\begin_inset Formula $G(x)\in B(x^{*},min(\eta,\widetilde{\eta}))$
\end_inset


\end_layout

